{
  "rewrites": [
    { "source": "/", "destination": "/api" },
    { "source": "/((?!api/).*)", "destination": "/api/$1" }
  ],
  "functions": {
    "api/health": {
      "includeFiles": "src/**"
    },
    "api/index": {
      "includeFiles": "src/**"
    },
    "api/[...path]": {
      "includeFiles": "src/**"
    }
  }
}


